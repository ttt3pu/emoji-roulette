    "svelte": "^3.46.4",
    "svelte-check": "^2.4.6",
    "svelte-preprocess": "^4.10.4",
    "tslib": "^2.3.1",
    "twemoji": "^14.0.1",
    "typescript": "^4.6.2",
    "vite": "^2.8.6"
  },
  "dependencies": {
    "npm-run-all": "^4.1.5",
